#!/bin/sh
# Set this flag to enable development code.
DEV=0
. ./lib_build
make base64
make basic
make buffer
make convert
make crypto
make die
make fexl
make file
make file_str
make format
make input
make limit
make memory
make nacl
make parse
make report
make sha256
make sha512
make standard
make str
make type_buf
make type_cmp
make type_crypto
make type_file
make type_hex
make type_input
make type_istr
make type_limit
make type_math
make type_num
make type_output
make type_parse
make type_rand
make type_run
make type_signal
make type_str
make type_sym
make type_time
make type_tuple
make type_var
make type_with
make value
if [ $DEV -eq 1 ]; then
make output
make output_str
make show
fi
link fexl
ln -fs ../bin/fexl ../bin/fexl0
