#!/bin/sh
. ./lib_build

# Build test.so
make_p test
make_so test

# Build show.so
make_p show
make_so show

# Build fexl.
make_o base64
make_o basic
make_o bn
make_o buf
make_o buf_str
make_o convert
make_o crypto
make_o die
make_o fexl
make_o file
make_o file_str
make_o format
make_o limit
make_o memory
make_o nacl
make_o parse
make_o report
make_o sha256
make_o sha512
make_o str
make_o stream
make_o type_bn
make_o type_buf
make_o type_cmp
make_o type_crypto
make_o type_file
make_o type_hex
make_o type_input
make_o type_istr
make_o type_limit
make_o type_load
make_o type_math
make_o type_num
make_o type_output
make_o type_parse
make_o type_rand
make_o type_record
make_o type_run
make_o type_signal
make_o type_stream
make_o type_str
make_o type_sym
make_o type_time
make_o type_tuple
make_o type_var
make_o type_with
make_o value
push_l m
push_l dl
make_e fexl

ln -fs ../bin/fexl ../bin/fexl0
