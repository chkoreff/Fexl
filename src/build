#!/bin/sh
. ./lib_build

# Build test.so
#[
make_p test
make_so test
#]

# Build core_20240529.so
#[
make_p limit
make_p type_buf
make_p type_cmp
make_p type_hex
make_p type_limit
make_p type_math
make_p type_rand
make_p type_run
make_p type_stream
make_p type_time
make_p type_var
make_p type_with
make_p core_20240529
# system lib math
push_l m
make_so core_20240529
#]

# Build show.so
#[
make_p show
push_so core_20240529
make_so show
#]

# Build fexl.
#[
make_o base64
make_o basic
make_o bn
make_o buf
make_o buf_str
make_o convert
make_o crypto
make_o die
make_o fexl
make_o file
make_o file_str
make_o format
make_o memory
make_o nacl
make_o parse
make_o report
make_o sha256
make_o sha512
make_o str
make_o stream
make_o type_bn
make_o type_crypto
make_o type_file
make_o type_input
make_o type_istr
make_o type_num
make_o type_output
make_o type_parse
make_o type_record
make_o type_signal
make_o type_str
make_o type_sym
make_o type_tuple
make_o value
# system lib dynamic library
push_l dl

# Used in development to call show.
#push_so show
#push_so core_20240529

make_e fexl
#]
