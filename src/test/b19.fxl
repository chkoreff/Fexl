)

(
say "= merge_keys"
# Test merge_keys.
\xs=[{"a" 1} {"b" 2} {"d" 4} {"g" 7}]
\ys=[{"a" -1} {"c" -3} {"d" -4} {"e" -5} {"f" -6}]
