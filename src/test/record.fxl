#
\try_keys==
	(
	\keys==["a" "b" "c" "d" "e"]
	\obj
	say ["count "(as_str; record_count obj)]
	each keys
		(\key
		\v=(obj key)
		say [key" = "(as_str v)]
		)
	nl
	)

