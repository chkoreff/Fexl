#
value; std; (value; std; use "test.fxl") \;

say "== test Fibonacci"

# (fib n) returns the nth Fibonacci number starting at 0.
\fib=
	(
	\memo=cache
	@\\fib\n
	le n 1 (eq n 1 1 0);
	memo n;
	+ (fib (- n 1)) (fib (- n 2))
	)

\try=
	(\x\y
	div
	each (range x y) \n
	say ["fib " n " = " (fib n)]
	)

show_benchmark;
try 8 0
try 54 73 # Show the last 20 that don't use "e" notation.
