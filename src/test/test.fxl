#
\div==(say "==")
\show_list=(\xs say "[" (each xs say) say "]")

\run_test=
	(\label\code
	say ["[ "label]
	code
	say "]"
	)

\do_benchmark=
	(\f
	fexl_benchmark f \val\steps\bytes
	trace ["steps "steps" bytes "bytes]
	val
	)

define "div" div;
define "show_list" show_list;
define "run_test" run_test;
define "do_benchmark" do_benchmark;
standard
