# Routines shared by test programs
use "test/lib.fxl" \;
\div=(say "==")
\show_list=(\xs say "[" (each xs say) say "]")

# (stream_values fn) yields the infinite list of values of function fn.  This
# is useful for generating streams of random values.
\stream_values=
	(\fn
	\list==
		(@\loop
		\x=;fn
		\xs=;loop
		cons x xs
		)
	yield list
	)

export;
def "div" div;
def "show_list" show_list;
def "stream_values" stream_values;
void
