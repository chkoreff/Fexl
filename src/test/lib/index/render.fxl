#
\render_index=
	(@\render_index
	render_list
		(\row\next
		row \is_leaf\key\val
		is_leaf
			(
			say ["{T "(fexl_quote key) " " (fexl_quote val) "}"];
			next
			)
			(
			say ["{F "(fexl_quote key) ];
			indent
				(
				render_index val;
				end
				);
			say "}";
			next
			)
		)
	)

\form
def "render_index" render_index;
form
