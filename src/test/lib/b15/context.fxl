#
\cx_render=(evaluate cx_std; use "lib/render/base.fxl")
\cx_render_list=(evaluate (chain cx_render; cx_std); use "lib/render/list.fxl")

\cx_render_local=
(
evaluate
(
chain cx_render_list;
chain cx_render;
cx_std
)
\;

\render_val=
	(\val
	is_defined val (fexl_quote val) "void"
	)

\say_pairs=
	(
	say_list
		(\row
		row \key\val
		say ["{" (fexl_quote key) " " (render_val val) "}"]
		)
	)

\say_errors=
	(
	say_list
		(\row
		row \key\val\val2
		say ["{" (fexl_quote key)" " (render_val val)" "(render_val val2)"}"]
		)
	)

define "say_pairs" say_pairs;
define "say_errors" say_errors;
void
)

\cx_render_index=
	(
	evaluate
	(
	chain cx_render_list;
	chain cx_render;
	cx_std
	);
	use "lib/index/render.fxl"
	)

\cx_output=(chain cx_render_index; cx_render_local)

\cx_index=(evaluate cx_std; use "lib/index/index.fxl")

chain cx_output;
cx_index
