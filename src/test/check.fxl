\test=
	(\name

	put ["test "name" "]

	\cmd=(to_str ["../bin/fexl test/show.fxl "name" | diff - out/"name])
	\argv=["/bin/sh" "-c" cmd]

	run_program argv ""
	\out\err\status

	eq 0 status
		(
		say "good"
		)
		(
		say "failed"
		put out
		put err
		)
	)

